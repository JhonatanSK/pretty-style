<div class="sign-up-address-container">
	<div class="container">
		<div class="col-md-12 header">
			<div class="row content">
				<div class="title">
					<label>Editar Endereço</label>
				</div>
				<div class="line">
					<hr />
				</div>
			</div>
		</div>
		<div class="jumbotron main">
			<div class="col-md-12 content">
				<div class="client-form container-fluid">
					<form>
						<div class="form-row form">
							<div class="form-group col-md-6">
								<label for="inputNomeDestinatario">Nome Do Destinatario
									<span style="color: #ff0000; margin-left: 5px;">*</span>
								</label> <input type="text" class="form-control"
									id="inputNomeDestinatario4"
									placeholder="Como você gostaria de ser chamado?"
									name="nomeDestinatario">
							</div>
							<div class="form-group col-md-6">
								<label for="inputCEP">CEP <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="text" class="form-control" id="inputCEP"
									placeholder="Ex: 12345-000" name="CEP">
							</div>
							<div class="form-group col-md-4">
								<label for="inputLogradouro">Logradouro <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="text" class="form-control" id="inputLogradouro"
									placeholder="Ex: Rua: ..." name="logradouro">

							</div>
							<div class="form-group col-md-2">
								<label for="inputEmail">Numero <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="number" class="form-control" id="inputNumero"
									placeholder="Ex: 28" name="numero">
							</div>
							<div class="form-group col-md-6">
								<label for="inputBairro">Bairro <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="email" class="form-control" id="inputBairro"
									placeholder="Ex: Vila Gomes Cardim" name="bairro">
							</div>
							<div class="form-group col-md-6">
								<label for="inputCidade">Cidade <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="text" class="form-control" id="inputCidade" name="cidade">
							</div>
							<div class="form-group col-md-6 ">
								<label for="inputEstado">Estado <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="text" class="form-control medium material-icons"
									id="inputEstado" name="estado">
							</div>
							<div class="form-group col-md-6">
								<label for="inputPais">Pais <span
									style="color: #ff0000; margin-left: 5px;">*</span></label> <input
									type="text" class="form-control" id="inputPais" name="pais">
							</div>
							<div class="form-group col-md-6">
								<label for="inputComplemento">Complemento (Opcional)</label> <input
									type="text" class="form-control" id="inputComplemento"
									name="Complemento">
							</div>
						</div>
					</form>
					<button type="submit" class="btn btn-primary center"
						style="position: absolute; border-radius: 70px;">Salvar</button>

				</div>

			</div>
		</div>
	</div>
</div>

